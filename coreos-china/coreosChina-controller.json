{
  "id": "coreosChinaController",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 2,
    "replicaSelector": {"name": "coreosChina"},
    "podTemplate": {
      "desiredState": {
         "manifest": {
           "version": "v1beta1",
           "id": "coreosChinaController",
           "containers": [
             {
               "name": "coreosChina",
               "image": "yuanxiaolong/coreos-china",
               "env": [
                  {
                    "key": "VIRTUAL_HOST", 
                    "value": "yuanxiaolong.cn,www.yuanxiaolong.cn"
                  }
               ]
             }
           ]
        }
      },
       "labels": {"name": "coreosChina"}
    }
  },
  "labels": {"name": "coreosChina"}
}